
html
  overflow-x hidden
::selection
  background #333
  color #f1f1f1
::-webkit-scrollbar {
  width: 10px;
}

::-webkit-scrollbar-track {
  background: rgb(170, 170, 170);
}

::-webkit-scrollbar-thumb {
  background: rgb(100, 100, 100);
  border-radius: 10px;
}

::-webkit-scrollbar-thumb:hover {
  background: rgb(100, 100, 100);
  border-radius: 10px;
}

::-webkit-scrollbar-thumb:active {
  background: rgb(68, 68, 68);
  border-radius: 10px;
}
ul
  list-style none
  padding 0
  margin 0
.text-right
  text-align right

.table-blue
  font-weight: 400;
  color: #3269F0;

.table-default
  font-weight: 400;
  color: #1D2157;
.table-wrap
  .thead >div, .trow >.tcell
    &:nth-of-type(1)
      text-align left
    &:last-of-type
      text-align right
    font-size: 12px;
    font-family: PingFangSC-Regular, PingFang SC;
    text-align center
    position relative
    width 25%
    white-space nowrap
    overflow hidden
    text-overflow ellipsis
  .thead>div
    font-weight: 500;
    color: #1D2157;
.thead
    width 100%
    height: 35px;
    background: #F7F9FC;
    display flex
    align-items center
    font-size: 12px;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #1D2157;
    padding 0 20px
    box-sizing border-box
  .trow
    display flex
    align-items center
    padding 0 20px
    box-sizing border-box
    padding-top 14px
